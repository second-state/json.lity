pragma lity ^1.2.4;

import "./json.lity";

contract Test {
	using JSON for JSON.ValueRef;
	function main(string raw) pure public returns(string) {
		JSON.ValueRef memory root = JSON.newValueRef(raw);
                require(root.arrayGet(raw, 0).getType(raw) == JSON.Type.BOOL);
                require(root.arrayGet(raw, 1).getType(raw) == JSON.Type.NUMBER);
                require(root.arrayGet(raw, 2).getType(raw) == JSON.Type.STRING);
                require(root.arrayGet(raw, 3).getType(raw) == JSON.Type.ARRAY);
                require(root.arrayGet(raw, 4).getType(raw) == JSON.Type.OBJECT);
                return raw;
	}
}
