pragma lity ^1.2.4;

import "./json.lity";

contract Test {
	using JSON for JSON.ValueRef;
	function main(string raw) pure public returns(string) {
		JSON.ValueRef memory root = JSON.newValueRef(raw);
		JSON.ValueRef memory a1 = root.arrayGet(raw, 1);
		require(a1.begin == 10);
		require(a1.end == 17);
		return a1.asString(raw);
	}
}
